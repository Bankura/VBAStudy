'******************************************************************************
Private Sub Class_Initialize()
End Sub

'******************************************************************************
'* メソッド定義
'******************************************************************************
'******************************************************************************
'* [概  要] StartProcess
'* [詳  細] 開始時の処理を行う。
'*
'******************************************************************************
Public Sub StartProcess(enumerator As Object)
    Debug.Print "■開始 --------------------------------"
End Sub

'******************************************************************************
'* [概  要] Edit
'* [詳  細] 行編集処理を行う。
'*
'******************************************************************************
Public Sub Edit(enumerator As Object)
    Dim msg As New StringEx
    msg.Append " 現在行　:" & enumerator.CurrentRow
    msg.Append " 開始行　:" & enumerator.StartRow
    msg.Append " 終了行　:" & enumerator.EndRow
    msg.Append " 処理件数:" & enumerator.CurrentReportCount
    msg.Append " 最大件数:" & enumerator.MaxReportCount
    Debug.Print msg.ToString
End Sub

'******************************************************************************
'* [概  要] EndProcess
'* [詳  細] 終了時の処理を行う。
'*
'******************************************************************************
Public Sub EndProcess(enumerator As Object)
    Debug.Print "■終了 --------------------------------"
End Sub

'******************************************************************************
'* インタフェース実装
'******************************************************************************
'*-----------------------------------------------------------------------------
'* IRowEditor#StartProcess
'*
'*-----------------------------------------------------------------------------
Private Sub IRowEditor_StartProcess(enumerator As Object)
    Me.StartProcess enumerator
End Sub
